program chastkova_suma_matrici; {$APPTYPE CONSOLE} type  tmas=array[1..1000,1..1000]of int64; var  mas,res:tmas;  n,m,i,j:longword; begin  assign(Input,'input.txt');  Reset(Input);  Assign(Output,'output.txt');  Rewrite(Output);  read(n,m);  for i := 1 to n do    for j := 1 to m do      read(mas[i,j]);  res:=mas;  for i := 2 to n do    res[i,1]:=res[i-1,1]+res[i,1];  for i := 2 to m do    res[1,i]:=res[1,i-1]+res[1,i];  for i := 2 to n do    for j := 2 to m do      res[i,j]:=res[i-1,j]+res[i,j-1]-res[i-1,j-1]+res[i,j];  for i := 1 to n do    begin      write(res[i,1]);      for j := 2 to m do        write(' ',res[i,j]);      writeln;    end;  Close(Input);  Close(Output); end.